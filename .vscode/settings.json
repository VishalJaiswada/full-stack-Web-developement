{
    "liveServer.settings.port": 5501,
    
    "window.zoomLevel": 3,
    "diffEditor.ignoreTrimWhitespace": false,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.mergeConfigurations": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.referenceOperator.showSpace": true,
    "C_Cpp.legacyCompilerArgsBehavior": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.ignoreSelection": true,
    "code-runner.showStopIconInEditorTitleMenu": false,
    "search-node-modules.useLastFolder": true,
    "npm.keybindingsChangedWarningShown": true,
    "npm.oldKeybindings.enable": true,
    "node-module-intellisense.autoStripExtensions": [

        ".js",
        ".jsx",
        ".ts",
        ".d.ts",
        ".tsx"
    ],
    "node-module-intellisense.modulePaths": [
        
    ]
}